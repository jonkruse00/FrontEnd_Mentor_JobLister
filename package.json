    "jest-cli": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "less": "^3.12.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svg-sprite": "^1.4.1",
    "react-test-renderer": "^17.0.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.{js,jsx}",
      "!**/node_modules/**",
      "!**/assets/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  },
  "svgSpriteOptions": {
    "include": [
      "./assets/**/*"
    ]
  }
}